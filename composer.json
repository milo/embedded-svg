{
    "name": "milo/embedded-svg",
    "description": "Latte macro to embed SVG from file into HTML",
    "license": "MIT",
    "authors": [
        {
            "name": "Miloslav HÅ¯la",
            "email": "miloslav.hula@gmail.com",
            "homepage": "https://github.com/milo"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-dom": "*",
        "nette/di": "^3.0.13",
        "latte/latte": "^3.0-RC"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.6.8",
        "symplify/easy-coding-standard": "^10.2",
        "phpunit/phpunit": "^9.5",
        "nette/bootstrap": "^3.1",
        "tracy/tracy": "^2.9",
        "nette/application": "^3.1",
        "symplify/easy-testing": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "Milo\\EmbeddedSvg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Milo\\EmbeddedSvg\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs --ansi",
        "fix-cs": "vendor/bin/ecs --ansi --fix",
        "phpstan": "vendor/bin/phpstan analyse"
    }
}
